# Django Environment Variables Template
# Copy this file to .env and fill in your actual values

# Database Configuration
# Leave DATABASE_URL commented out to use SQLite for local development
# Uncomment and fill in to use PostgreSQL locally (optional)
# DATABASE_URL=postgresql://username:password@ep-xxx.region.aws.neon.tech/dbname

# Django Settings
DEBUG=True
SECRET_KEY=your-secret-key-here

# Django Admin Configuration
DJANGO_ADMIN_URL=admin-dev/

# CORS Configuration for React Frontend
FRONTEND_URL=http://localhost:5173

# Environment Indicator
ENVIRONMENT=development