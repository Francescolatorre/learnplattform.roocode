{
  "name": "learnplatform-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:watch": "vitest watch",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "build": "npm run lint && vite build",
    "test:coverage": "vitest run --coverage --mode=ci --passWithNoTests",
    "test:ci": "vitest run --coverage --mode=ci --passWithNoTests",
    "deps:graph": "depcruise --validate .dependency-cruiser.js --output-type dot src --exclude 'node_modules'| dot -T svg > dependency-graph.svg",
    "deps:validate": "depcruise --validate .dependency-cruiser.js src ",
    "deps:json": "depcruise --output-type json src --exclude 'node_modules' > dependency-graph.json"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^5.0.1",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@mui/x-data-grid": "^7.28.2",
    "@tanstack/react-query": "^5.70.0",
    "axios": "^1.6.7",
    "chart.js": "^4.4.8",
    "date-fns": "^4.1.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "formik": "^2.4.6",
    "lodash": "^4.17.21",
    "quill": "^2.0.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.55.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.22.1",
    "react-syntax-highlighter": "^15.6.1",
    "react-transition-group": "^4.4.5",
    "yup": "^1.6.1",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@playwright/test": "^1.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.17.30",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.0.4",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^16.10.1",
    "eslint": "^9.25.0",
    "eslint-define-config": "^2.1.0",
    "eslint-import-resolver-typescript": "^4.3.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-react": "^7.37.4",
    "fs": "^0.0.1-security",
    "glob": "^11.0.1",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "openapi-typescript-codegen": "^0.29.0",
    "path": "^0.12.7",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
